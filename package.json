{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.1.3",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "dependencies": {
    "cli-engine": "0.0.1",
    "heroku-apps": "2.1.24",
    "heroku-certs": "1.1.26",
    "heroku-cli-addons": "1.2.8",
    "heroku-cli-command": "0.1.4",
    "heroku-cli-test": "0.0.2",
    "heroku-fork": "4.1.14",
    "heroku-git": "2.5.10",
    "heroku-local": "5.1.8",
    "heroku-orgs": "1.5.5",
    "heroku-pg": "2.0.16",
    "heroku-pipelines": "1.3.1",
    "heroku-redis": "1.2.8",
    "heroku-run": "3.4.3",
    "heroku-spaces": "2.6.0",
    "heroku-status": "3.0.5"
  },
  "cli-engine": {
    "name": "heroku-cli",
    "bin": "heroku",
    "plugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-cli-addons",
      "heroku-fork",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-status"
    ]
  },
  "scripts": {
    "release": "echo $1 $2"
  },
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "main": "cli.js",
  "repository": "https://github.com/heroku/cli"
}
