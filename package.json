{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.11.17",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "dirname": "heroku",
    "bin": "heroku",
    "node": "7.10.0",
    "defaultCommand": "dashboard",
    "cloudfront": "EHF9FOCUJYVZ",
    "s3": {
      "bucket": "heroku-cli-assets",
      "host": "cli-assets.heroku.com"
    },
    "plugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-ci",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-cli-status",
      "heroku-fork",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-ps-exec",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces"
    ]
  },
  "dependencies": {
    "cli-engine": "3.5.35",
    "heroku-apps": "2.4.2",
    "heroku-certs": "1.1.40",
    "heroku-ci": "1.8.2",
    "heroku-cli-addons": "1.2.22",
    "heroku-cli-oauth": "2.0.13",
    "heroku-cli-status": "4.0.0",
    "heroku-fork": "4.1.24",
    "heroku-git": "2.5.18",
    "heroku-local": "5.1.17",
    "heroku-orgs": "1.6.3",
    "heroku-pg": "2.2.4",
    "heroku-pipelines": "2.1.2",
    "heroku-ps-exec": "1.0.5",
    "heroku-redis": "1.2.14",
    "heroku-run": "3.4.12",
    "heroku-spaces": "2.9.4",
    "semver": "5.3.0"
  },
  "devDependencies": {
    "heroku-plugin-readme-generator": "git+ssh://git@github.com/heroku/heroku-cli-dev-center-generator.git#flags-as-tables"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=7.10.0"
  },
  "files": [
    "bin"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "repository": "https://github.com/heroku/cli",
  "scripts": {
    "build": "cli-build",
    "release": "cli-release"
  }
}
